{
    "name": "cleeng/svod-demo",
    "type": "project",
    "homepage": "https://github.com/zendframework/zend-expressive-skeleton",
    "license": "BSD-3-CLAUSE",
    "authors": [
        {
            "name": "Tomasz Szadkowski",
            "homepage": "http://cleeng.com"
        },
        {
            "name": "Mateusz Tymek",
            "homepage": "http://mateusztymek.pl"
        }
    ],
    "require": {
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "1.0.0rc5",
        "zendframework/zend-stdlib": "~2.7",
        "zendframework/zend-expressive-fastroute": "^1.0",
        "zendframework/zend-servicemanager": "^2.5",
        "ocramius/proxy-manager": "^1.0",
        "zendframework/zend-expressive-zendviewrenderer": "^1.0",
        "cleeng/cleeng-php-sdk": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
    }
}
